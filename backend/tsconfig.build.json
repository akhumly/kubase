{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "declaration": false,
    "sourceMap": true,
    "removeComments": true,
    "strict": true,
    "noImplicitAny": false
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "test", "src/**/*.spec.ts"]
}
